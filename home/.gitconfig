[user]
	name = Jared Pace
	email = jared@codeword.io
[color]
   branch = auto
   diff = auto
   status = auto
[color "branch"]
   current = yellow reverse
   local = yellow
   remote = green
[color "diff"]
   meta = yellow bold
   frag = magenta bold
   old = red bold
   new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  st = status
  co = checkout
  ci = commit
  cp = cherry-pick
[github]
	user = jdpace
[push]
	default = tracking
[branch]
	autosetuprebase = remote
[core]
	autocrlf = input
	excludesfile = ~/.gitignore_global
[credential]
	helper = osxkeychain
